<div class="container-fluid">
	<div class="row">
		<div class="col-xs-12">
			<table class="table table-hover">
				<tr class="info">
					<th class="text-center">Participante</th>
					<th class="text-center">Tipo Inscripci√≥n</th>
					<th class="text-center">Documento de Identidad</th>
					<th class="text-center">Fecha de Nacimiento</th>
					<th class="text-center">EPS</th>
					<th class="text-center">Email</th>
					<th class="text-center">Club</th>
				</tr>
				<% @registers.each do |register| %>
					<tr class="text-center">
						<td class="text-left"><%= register.first_name %> <%= register.second_name %> <%= register.first_last_name %> <%= register.second_last_name %></td>
						<td><%= register.register_type.name %></td>
						<td><%= register.identification %></td>
						<td><%= register.date_of_birth %></td>
						<td><%= register.eps %></td>
						<td><%= register.email %></td>
						<td><%= Club.find(Inscription.find(register.inscription_id).club_id).name %></td>
					</tr>					
				<% end %>
			</table>
			<div class="row">
				<% @registers.each do |register| %>
					<%= image_tag register.avatar.url(:thumb) %>			
				<% end %>				
			</div>
		</div>
	</div>
</div>